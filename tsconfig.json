{
    "compilerOptions": {
      "module": "NodeNext",
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "outDir": "out",
      "sourceMap": true,
      "rootDir": "src",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "downlevelIteration": true,
      "jsx": "react"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", ".vscode-test"]
  }