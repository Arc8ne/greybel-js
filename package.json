{
    "name": "greybel-js",
    "version": "1.0.5",
    "engines": {
        "node": ">=14.0.0"
    },
    "description": "Transpiler/Interpreter for GreyScript. (GreyHack)",
    "homepage": "https://github.com/ayecue/greybel-js",
    "repository": {
        "type": "git",
        "url": "git@github.com:ayecue/greybel-js.git"
    },
    "author": {
        "name": "ayecue",
        "email": "soerenwehmeier@googlemail.com"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/ayecue/greybel-js/blob/master/LICENSE-MIT"
        }
    ],
    "dependencies": {
        "@babel/runtime": "^7.16.7",
        "@codemirror/basic-setup": "^0.18.2",
        "@codemirror/commands": "^0.18.3",
        "@codemirror/lang-javascript": "^0.18.0",
        "@codemirror/view": "^0.18.17",
        "commander": "^7.2.0",
        "greybel-core": "^0.2.4",
        "greybel-gh-mock-intrinsics": "^0.2.6",
        "greybel-interpreter": "^0.6.3",
        "greybel-intrinsics": "^0.5.2",
        "greybel-transpiler": "^0.2.4",
        "greyscript-core": "^0.2.5",
        "inquirer": "^8.1.0",
        "inquirer-command-prompt": "^0.1.0",
        "json-formatter-js": "^2.3.4",
        "localstoragedb": "git+https://github.com/ayecue/localStorageDB.git",
        "mkdirp": "^1.0.4",
        "open": "^8.2.1",
        "semver": "^5.3.0",
        "uuid": "^8.3.2"
    },
    "bugs": {
        "url": "https://github.com/ayecue/greybel-js/issues"
    },
    "bin": {
        "greybel": "./bin/compile",
        "greybel-repl": "./bin/repl",
        "greybel-execute": "./bin/execute",
        "greybel-ui": "./bin/ui"
    },
    "scripts": {
        "prepublishOnly": "npm run compile",
        "compile": "npm run compile:local && npm run compile:web && npm run move:web",
        "move:web": "cp -a web/. out/",
        "compile:local": "tsc -p ./",
        "compile:web": "rollup --config rollup.config.js",
        "watch": "tsc -watch -p ./"
    },
    "directories": {},
    "keywords": [
        "greybel",
        "greyscript",
        "greyhack",
        "grey hack",
        "grey",
        "hack",
        "miniscript",
        "unity",
        "lexer",
        "parser",
        "compiler",
        "transpiler",
        "emulator",
        "interpreter"
    ],
    "devDependencies": {
        "@babel/core": "^7.16.10",
        "@babel/helpers": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.16.8",
        "@babel/preset-env": "^7.16.8",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@types/inquirer": "^8.1.3",
        "@types/mkdirp": "^1.0.2",
        "@types/node": "^12.12.0",
        "@types/uuid": "^8.3.4",
        "@types/vscode": "^1.63.1",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "@vscode/test-electron": "^2.0.3",
        "rollup": "^2.64.0",
        "rollup-plugin-polyfill-node": "^0.8.0",
        "rollup-plugin-terser": "^7.0.2",
        "source-map-support": "^0.5.12",
        "typescript": "^4.5.4"
    }
